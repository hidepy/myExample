{
  "name": "getTabUrls",
  "manifest_version": 2,
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "src/js/jquery.min.js",
        "src/js/content_script.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "src/js/background.js"
    ]
  },
  "permissions": [
    "<all_urls>",
    "tabs"
  ],
  "web_accessible_resources": [
    "src/js/*"
  ]
}